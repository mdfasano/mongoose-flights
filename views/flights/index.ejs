<%- include('../partials/header') %>

<h2>Flight List</h2>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>flightNo</th>
      <th>departs</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach((flight) => { %>
      <tr>
        <td><%= flight.airline %></td>
        <td><%= flight.airport %></td>
        <td><%= flight.flightNo %></td>
        <td><%= flight.departs %></td>
      </tr>
    <%  }); %>
  </tbody>
</table>

<%- include('../partials/header') %>